{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0.0.0",
  "id": "",
  "packageName": "com.microsoft.scrumstatus",
  "developer": {
    "name": "PwCLabsStrategy",
    "websiteUrl": "https://github.pwc.com/antoinette-d-biagi/gizmonic",
    "privacyUrl": "https://github.pwc.com/antoinette-d-biagi/gizmonic",
    "termsOfUseUrl": "https://github.pwc.com/antoinette-d-biagi/gizmonic"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "StandupBot"
  },
  "description": {
    "short": "StandupBot helps you get status updates from your team in group chat.",
    "full": "StandupBot is a simple scrum assistant that will enable users to run asynchronous standup meetings and provide and easy way for the users to share their daily updates."
  },
  "accentColor": "#FFCCD8",
  "bots": [
    {
      "botId": "",
      "scopes": [
        "groupchat"
      ],
      "commandLists": [
        {
          "scopes": [
            "groupchat"
          ],
          "commands": [
            {
              "title": "Start scrum",
              "description": "Enter and review updates from the people in this group chat"
            },
            {
              "title": "End scrum",
              "description": "Stop the current scrum"
            }
          ]
        }
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "StandupBotPwCLabsStrategy.azurewebsites.net"
  ]
}